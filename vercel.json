{
    "version": 2,
    "builds": [
      { "src": "app/main.py", "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }}
    ],
    "buildCommand": "uvicorn app:main",
    "installCommand": "pip install -r --upgrade requirements.txt",
    "rewrites": [
      { "source": "/(.*)", "destination": "main.py" }
    ]
  }